{
  "name": "to-path-tree",
  "version": "0.2.0",
  "description": "Convert a file path array to a tree.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/to-path-tree.git"
  },
  "keywords": [
    "file path",
    "path",
    "tree",
    "file paths to tree"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "prepare": "simple-git-hooks",
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "release": "vitest run && bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.43.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "lint-staged": "^15.0.2",
    "pathe": "^1.1.1",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yaml,yml}": [
      "eslint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-ts"
  }
}
